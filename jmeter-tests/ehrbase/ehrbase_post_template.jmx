                      &lt;items id=&quot;text&quot;&gt;Authorisation expiry date&lt;/items&gt;&#xd;
                    &lt;/term_definitions&gt;&#xd;
                    &lt;term_definitions code=&quot;at0073&quot;&gt;&#xd;
                      &lt;items id=&quot;description&quot;&gt;Whether the medication is only issued once or may re-issued and dispensed &apos;re-filled&apos; after authorisation by the prescriber.&lt;/items&gt;&#xd;
                      &lt;items id=&quot;text&quot;&gt;Authorisation type&lt;/items&gt;&#xd;
