                      &lt;items id=&quot;text&quot;&gt;No repeat supply&lt;/items&gt;&#xd;
                    &lt;/term_definitions&gt;&#xd;
                    &lt;term_definitions code=&quot;at0075&quot;&gt;&#xd;
                      &lt;items id=&quot;description&quot;&gt;Multiple refills of the prescription may be obtained from the prescriber.&lt;/items&gt;&#xd;
                      &lt;items id=&quot;text&quot;&gt;Repeat prescribing&lt;/items&gt;&#xd;
                    &lt;/term_definitions&gt;&#xd;
                    &lt;term_definitions code=&quot;at0076&quot;&gt;&#xd;
